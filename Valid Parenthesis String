class Solution:
    def checkValidString(self, s: str) -> bool:
        minimum=maximum=0
        for i in s:
            if i=='(':
                minimum+=1
                maximum+=1
            elif i==')':
                mininimum=max(0,minimum-1)
                maximum-=1
            else:
                mininimum=max(0,minimum-1)
                maximum+=1
        if maximum<0:
            return False
        else:
            return True
